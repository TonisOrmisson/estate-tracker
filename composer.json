{
    "name": "tonisormisson/estate-tracker",
    "description": "Yii 2 Basic Project Template implementing Yii2-user",
    "keywords": ["yii2", "framework", "basic", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "repositories": [
        {
            "type" : "composer",
            "url" : "https://packages.andmemasin.eu/"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }

    ],
    "prefer-stable":true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.5",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "dektrium/yii2-user":"0.9.*",
        "andmemasin/helpers": "dev-master",
        "andmemasin/yii2-json-form": "1.2.*",
        "kartik-v/yii2-widget-select2": "@dev",
        "kartik-v/yii2-widget-switchinput": "*",
        "miloschuman/yii2-highcharts-widget": "~5.0"


    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0"

    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
