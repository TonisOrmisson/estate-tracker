{
    "name": "tonisormisson/yii2-user-template",
    "description": "Yii 2 Basic Project Template implementing Yii2-user",
    "keywords": ["yii2", "framework", "basic", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "proprietary",
    "minimum-stability": "dev",
    "repositories": [
      {"url": "https://t6nnp6nn@bitbucket.org/andmemasin/yii2-json-form.git", "type": "vcs"},
      {"url": "https://t6nnp6nn@bitbucket.org/andmemasin/helpers.git", "type": "vcs"}

    ],
    "prefer-stable":true,
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.5",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "dektrium/yii2-user":"0.9.*",
        "andmemasin/helpers": "dev-master",
        "andmemasin/yii2-json-form": "1.2.*",
        "kartik-v/yii2-widget-select2": "@dev",
        "miloschuman/yii2-highcharts-widget": "*"


    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3"
    },
    "config": {
        "process-timeout": 1800
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ],
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        },
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
